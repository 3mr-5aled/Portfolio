    // Projects section
    section#projects.projects
        h2.title Projects
        .content
            - var reversedPosts1 = collections.post.reverse()    
            - var reversedPosts = reversedPosts1.reverse()    
            for post in reversedPosts
                .project-card.hidden.cardImage
                    .imageOne.projectImage 
                        img(src=post.data.img alt=post.data.title)
                    .project-info
                        p.project-category !{post.data.tag}
                        strong.project-title
                            span !{post.data.title}
                            a.more-details(href=post.data.link) More Details

    section#contact.cards.contact
        h2.title Contact
        .contactForm.hidden
            h1 Let&apos;s Talk
            form.form(action='https://formsubmit.io/send/5edcd4cc-74fc-4787-8aee-79d9f4c8fade' method='POST')
                input#name(name='_redirect' type='hidden' value='https://3mr5aled.netlify.app/formsubmission.html')
                input(type='text' name='Name' placeholder='Full Name' required='')
                input(type='email' name='Email' placeholder='Email' required='')
                // spam fooler
                input(name='_formsubmit_id' type='text' style='display:none')
                input(type='text' name='Subject' placeholder='Subject' required='')
                textarea(name='Message' placeholder='Message' required='')
                button(type='submit') Send

    script.
